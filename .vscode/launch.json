  "version": "0.2.0",
  "configurations": [
    {
      "request": "launch",
      "name": "Start API",
      "type": "node",
      "program": "${workspaceFolder}/src/main.ts",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "/home/edixon/.deno/bin/deno",
      "runtimeArgs": [
        "run",
        "--config",
        "./deno.jsonc",
        "--inspect-wait",
        "--allow-env",
        "--allow-read",
        "--allow-net"
      ],
      "attachSimplePort": 9229
    },
    {
      "request": "launch",
      "name": "Start Scraping",
      "type": "node",
      "program": "${workspaceFolder}/src/scraping/jobs.ts",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "/home/edixon/.deno/bin/deno",
      "runtimeArgs": [
        "run",
        "--config",
        "./deno.jsonc",
        "--inspect-wait",
        "--allow-env",
        "--allow-read",
        "--allow-net",
        "--allow-write"
      ],
      "attachSimplePort": 9229
    }
  ]
}
